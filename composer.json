{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": "^5.5",
		"laravel/framework": "5.3.*",
		"laravelcollective/html": "~5.3",
		"barryvdh/laravel-debugbar": "^2.0",
		"illuminate/html": "5.0.*",
		"maatwebsite/excel": "~2.1.0",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"

	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			
		],
		"pre-update-cmd": [
        		
        	],
		"post-update-cmd": [
			
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
